{
  "name": "take-home-edify-sde-full-stack-mid-level",
  "module": "index.ts",
  "type": "module",
  "license": "GPL-V3",
  "scripts": {
    "lint": "bun run biome check --write ./",
    "dev": "NODE_ENV=development bun --watch src/graphql-api/main.ts",
    "start": "NODE_ENV=production bun src/graphql-api/main.ts",
    "test": "NODE_ENV=test bun test",
    "db:create": "NODE_ENV=development bun run ./src/scripts/create-database.ts",
    "db:push": "bun run db:create && NODE_ENV=development drizzle-kit push --force",
    "docker:down": "docker-compose down --volumes",
    "docker:build": "docker-compose build",
    "docker:run": "docker-compose up",
    "docker:rebuild-run": "docker-compose down --volumes && docker-compose build && docker-compose up",
    "docker:postgres-only": "docker compose up db"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/bun": "latest",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@biomejs/biome": "^2.1.4",
    "@nestjs/config": "^4.0.2",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mercurius": "^13.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.6",
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "drizzle-orm": "^0.44.4",
    "graphql": "^16.11.0",
    "mercurius": "^16.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {},
  "trustedDependencies": [
    "@nestjs/core"
  ]
}
